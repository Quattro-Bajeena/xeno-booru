{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppOptions": {
    "StorageUrl": "https://xenoboorustorage.blob.core.windows.net",
    "PostContainer": "posts",
    "ResourceContainer": "resources",
    "AudioThumbnailFileName": "audio_cover.jpg",
    "AuthenticationRequired": {
      "UploadPost": false,
      "UpdatePost": true,
      "DeletePost": true,
      "CreatePool": true,
      "AddPoolEntry": true
    }
  }
}